{
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
			"filetypes": ["go"],
			"initializationOptions": {
				"usePlaceholders": true
			}
		},
	},
	"yaml.schemas": {
		"kubernetes": "/*.yaml"
	},
	"coc.preferences.hoverTarget": "float",
	"suggest.floatEanble": true
}
